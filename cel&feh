import java.util.*;
import java.lang.Math;
public class program4{
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
     
      
      System.out.println("enter temp in celsius:");
      double c =sc.nextInt();
     double f=(c*9/5)+32;
      
      System.out.println("temp in fehrenheit="+f);
     
     
    }

}
