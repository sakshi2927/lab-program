import java.util.Scanner;


public class program13 {
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the number");

        int n=sc.nextInt();
        int m=1;
        for(int i=1;i<=n;i++){
            
            m=m*i;
           
        }

        System.out.println("the factorial is"+m);
    }
    
}
